    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.8",
    "prettier": "^3.3.3",
    "stylelint": "^16.8.1",
    "stylelint-prettier": "^5.0.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.2.2",
